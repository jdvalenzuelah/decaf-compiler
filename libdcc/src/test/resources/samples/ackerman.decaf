{

	int i;
	int j;
	i = 0;
	while(i<10)
	{
		A[i]=InputInt();
		i = i+1;
	}
	i = 0;
	while (i<10)
	{
		OutputInt(Ackerman(A[i],j));
		i = i+1;
	}

	return;

}

int Ackerman(int m, int n)
{
	if (m==0)
	{ return n + 1; }
	else
	{
		if (n==1)
		{ return Ackerman(m + 1, 0); }
		else
		{ return Ackerman(m-1, Ackerman(m,n-1));}
		return 0;
	}
	return 0;

}

